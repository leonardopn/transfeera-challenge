generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model Receiver {
    id             Int    @id @default(autoincrement())
    completed_name String
    email          String
    cpf_cnpj       String
    status         String @default("Rascunho")
    pix_key_type   String
    pix_key        String
}
